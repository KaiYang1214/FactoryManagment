{
  "name": "simple_react_start_kit_2017",
  "description": "react webpack2 react-router example",
  "version": "2.0.0",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build:css": "NODE_ENV=production postcss src/tailwind.css -o src/index.css",
    "watch:css": "NODE_ENV=development postcss src/tailwind.css -o src/index.css -w",
    "postinstall": "npm run build:css",
    "prestart": "npm run watch:css & ",
    "start": "webpack-dev-server --mode development --config webpack.config.js",
    "dist": "npm run clean && npm run build:css && webpack --mode production --config webpack.config.js",
    "deploy": "npm run dist && gh-pages -d dist",
    "build:docker": "npm run dist && docker build -t reactdocker .",
    "lint": "eslint ./src --quiet",
    "lint:fix": "eslint ./src --fix",
    "test": "jest --config=jest.config.json --coverage",
    "test:f": "jest --detectOpenHandles --config=jest.config.json",
    "test:fw": "jest --coverage --watch --detectOpenHandles --config=jest.config.json",
    "analysis": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "i18n:update": "node ./src/locales/__generator__/__genI18nFile.js"
  },
